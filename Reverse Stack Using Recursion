void    insertatBottem(stack<int> &s,int element){
    if(s.empty()){
        s.push(element);
       return;
    }
      int num = s.top();
       s.pop();
       insertatBottem(s, element);
        s.push(num);
}
void reverseStack(stack<int> &stack) {
    // base case
    if(stack.empty()){
        return;
    }
    int num = stack.top();
    stack.pop();
    // recursiver call
    reverseStack(stack);
    
    insertatBottem( stack,num);
    
}
